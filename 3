public class Fclasss{
    private int[] myArray = new int[20];
    public int this[int ind1]{
        get{ return myArray[ind1]; }
        set{ myArray[ind1] = value; }
    }
    private string[] imyArray = new string['Hallo'];
    public string this[string ind2]{
        get{ return imyArray[ind2]; }
        set{ imyArray[ind2] = value; }
    }
    

     
}

public class C
{
    static void Main(string[] args){
        Fclasss<int> n = new Fclasss<int>();
        Fclasss<string> n2 = new Fclasss<string>();
    }
}