using System;
					
public class Program
{
	public static void Main()
	{
		A obj = new A();
		Console.WriteLine(obj.c);
	
	}
}
 class A
 {
	 private float a = 3.7f;
	 private float b = 6.4f;
	 
	 public float c
	 {
		 get {return a*=b;
			  c++;
			  a/b;
			 }
	 }
	 
	 public class B: A
	 {
		 public B(float d): base(d) {} public B():
		 for(int i=0; i<10; i++) Console.Write(i);
	 }}
