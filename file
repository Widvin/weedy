using System;
					
public class Program
{
	public static void Main()
	{	
		private int[] ar=new int[6];

				
	}

	public B(int length, int initialValue) {
      
        for (int i = 0; i < 6; i++)
            ar[i] = initialValue * i;
    }
    
    public B() : this(5,3) {}
    
    public void Print()
    {
        foreach (int number in array)
            Console.WriteLine(number);
	}
	}
		
